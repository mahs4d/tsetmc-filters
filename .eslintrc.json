{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "warn",
        "no-restricted-syntax": "off",
        "no-continue": "off",
        "no-await-in-loop": "off"
    }
}
